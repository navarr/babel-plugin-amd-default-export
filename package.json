  "description": "A Babel plugin to make the generated AMD modules export the default export directly and not wrapped in an object with a default property.",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/babel-plugin-amd-default-export/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/babel-plugin-amd-default-export#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prantlf/babel-plugin-amd-default-export.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/babel-plugin-amd-default-export/issues"
  },
  "engines": {
    "node": ">=6"
  },
